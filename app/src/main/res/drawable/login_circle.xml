<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorPrimaryTrans" />
            <stroke
                android:width="3dip"
                android:color="@color/midnightBlue" />
            <size
                android:width="90dp"
                android:height="90dp" />
        </shape>
    </item>
    <item
        android:top="15dp"
        android:left="15dp"
        android:right="15dp"
        android:bottom="15dp"
        android:drawable="@drawable/student"/>

</layer-list>